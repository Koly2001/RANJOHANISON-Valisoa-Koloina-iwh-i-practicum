doctype html
html
  head
    title Liste des Pets
  body
    h1 Liste des Pets
    table
      thead
        tr
          th Name
          th Type
          th Age
      tbody
        each obj in customObjects
          tr
            td= obj.Name
            td= obj.Type
            td= obj.Age
    p
      a(href='/update-cobj') Ajouter/Modifier ce tableau


